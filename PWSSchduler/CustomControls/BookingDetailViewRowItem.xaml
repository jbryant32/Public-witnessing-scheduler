<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PWSSchduler.CustomControls.BookingDetailViewRowItem"
             x:Name="This"
             >
  <ContentView.Content>
      <StackLayout >
          <Label Text="{Binding Source={x:Reference This},Path=Title}" TextColor="Gray" />
           <Label Text="{Binding Source={x:Reference This},Path=Info}"  TextColor="Black"/>
        </StackLayout>
  </ContentView.Content>
</ContentView>